package ch4;

public class ex07 {
//1부터 45까지의 수 중에 랜덤하게 6개를 추출해라
	public static void main(String[] args) {
			//1부터 45까지
		int[]num = new int[6];
		
		for (int i=0; i<num.length; i++)
			num[i] = (int)(Math.random()*50);  //1부터 50까지
		
			
			
		for (int i=0; i<num.length; i++)     
			System.out.print(num[i] + ", ");	
			
		
	}

}

//만약 2번째와 4번째가 같다면, 실행하지 않고 넘어가야 한다 ?